Cell[TextData[{
 StyleBox["Recuerda el enunciado del teorema fundamental para curvas en el \
espacio. La demostraci\[OAcute]n est\[AAcute] basada en la existencia y \
unicidad de soluci\[OAcute]n de un sistema de ecuaciones diferenciales \
ordinarias de primer orden. Dicho sistema puede resolverse con ",
  FontFamily->"Book Antiqua"],
 StyleBox["Mathematica",
  FontFamily->"Book Antiqua",
  FontSlant->"Italic"],
 StyleBox[" para dibujar curvas en el espacio que tengan una curvatura y una \
torsi\[OAcute]n prefijadas. Para ello utilizaremos comandos de resoluci\
\[OAcute]n num\[EAcute]rica de ecuaciones diferenciales. Veamos un ejemplo:\n\
",
  FontFamily->"Book Antiqua"]
}], "Text",
 CellChangeTimes->{{3.4976884661963177`*^9, 3.497688671059916*^9}, {
  3.504270046262623*^9, 3.5042701574238157`*^9}, {3.5042702196833277`*^9, 
  3.5042702249164*^9}, {3.504336880030843*^9, 3.5043368812120533`*^9}, {
  3.667042904948084*^9, 3.667042908208602*^9}},
 FontSize->16],

Cell["\<\
Curva con curvatura k(s) = 3 y torsi\[OAcute]n t(s) = 1 (obviamente debe \
obtenerse una h\[EAcute]lice circular):\
\>", "Text",
 CellChangeTimes->{{3.4723996884801273`*^9, 3.472399692847352*^9}, {
  3.472449851301713*^9, 3.472449859146867*^9}, {3.530257610397932*^9, 
  3.53025761517173*^9}, {3.6670429267624063`*^9, 3.667042936992449*^9}},
 FontFamily->"Book Antiqua",
 FontSize->16,
 FontWeight->"Plain",
 FontVariations->{"CompatibilityType"->0}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"k", "[", "s_", "]"}], ":=", "3"}], ";", 
  RowBox[{
   RowBox[{"t", "[", "s_", "]"}], ":=", "1"}]}]], "Input",
 CellChangeTimes->{{3.504288311602007*^9, 3.504288354803933*^9}, {
  3.5043308290319443`*^9, 3.504330835025054*^9}, {3.504338539458712*^9, 
  3.504338579442575*^9}, {3.530257617218849*^9, 3.530257621116283*^9}},
 FontSize->16],

Cell[BoxData[
 RowBox[{
  RowBox[{"curva", "=", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"x", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{"t1", "[", "s", "]"}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"y", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{"t2", "[", "s", "]"}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"z", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{"t3", "[", "s", "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"t1", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"k", "[", "s", "]"}], 
         RowBox[{"n1", "[", "s", "]"}]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"t2", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"k", "[", "s", "]"}], 
         RowBox[{"n2", "[", "s", "]"}]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"t3", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"k", "[", "s", "]"}], 
         RowBox[{"n3", "[", "s", "]"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"n1", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           RowBox[{"k", "[", "s", "]"}]}], 
          RowBox[{"t1", "[", "s", "]"}]}], "-", 
         RowBox[{
          RowBox[{"t", "[", "s", "]"}], 
          RowBox[{"b1", "[", "s", "]"}]}]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"n2", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           RowBox[{"k", "[", "s", "]"}]}], 
          RowBox[{"t2", "[", "s", "]"}]}], "-", 
         RowBox[{
          RowBox[{"t", "[", "s", "]"}], " ", 
          RowBox[{"b2", "[", "s", "]"}]}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"n3", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           RowBox[{"k", "[", "s", "]"}]}], " ", 
          RowBox[{"t3", "[", "s", "]"}]}], "-", 
         RowBox[{
          RowBox[{"t", "[", "s", "]"}], " ", 
          RowBox[{"b3", "[", "s", "]"}]}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"b1", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"t", "[", "s", "]"}], " ", 
         RowBox[{"n1", "[", "s", "]"}]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"b2", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"t", "[", "s", "]"}], " ", 
         RowBox[{"n2", "[", "s", "]"}]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"b3", "'"}], "[", "s", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"t", "[", "s", "]"}], " ", 
         RowBox[{"n3", "[", "s", "]"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"x", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"y", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"z", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"t1", "[", "0", "]"}], "\[Equal]", "1"}], ",", 
       RowBox[{
        RowBox[{"t2", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"t3", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"n1", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"n2", "[", "0", "]"}], "\[Equal]", "1"}], ",", 
       RowBox[{
        RowBox[{"n3", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"b1", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"b2", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{"b3", "[", "0", "]"}], "\[Equal]", "1"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
